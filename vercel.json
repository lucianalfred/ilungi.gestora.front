{
  "builds": [
    { 
      "src": "package.json", 
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
   "rewrites": [
    {
      "source": "/api/:match*",
      "destination": "http://213.199.62.60:8083/:match*"
    }
  ]
}
